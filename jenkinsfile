pipeline {
    agent any
    stages {
        stage("Build"){
            steps{
                sh "docker-compose up --build"
            }
        }
        stage('Deploy'){
            steps{
                sh "version=$(grep 'version:' pubspec.yaml | awk '{print $2}')"
                sh "echo $version > wms_mobile_versions.txt"
            }
        }
    }
}  